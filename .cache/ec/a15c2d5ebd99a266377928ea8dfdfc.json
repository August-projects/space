n\r\nheader {\r\n    background-color: black;\r\n    padding: 0 40px;\r\n\r\n    h1 {\r\n        @include text-indent;\r\n        width: 100px;\r\n        height: 80px;\r\n        background: url(./img/8th_logo_white.png) center / 60px 50px no-repeat;\r\n    }\r\n\r\n    nav {\r\n        @include grid;\r\n        @include center(space-between);\r\n        margin: auto;\r\n    }\r\n\r\n    ul {\r\n        display: flex;\r\n        width: fit-content;\r\n        justify-content: right;\r\n    }\r\n\r\n    a::after {\r\n        content: \"\";\r\n        position: absolute;\r\n        bottom: -5px;\r\n        left: 0;\r\n        width: 0;\r\n        height: 1.5px;\r\n        background-color: aliceblue;\r\n        border-radius: 50%;\r\n        transition: .2s ease-in;\r\n    }\r\n\r\n    li {\r\n        padding: 1em 2em;\r\n\r\n        &:hover a::after {\r\n            width: 100%;\r\n        }\r\n    }\r\n\r\n    a {\r\n        position: relative;\r\n        color: aliceblue;\r\n        text-decoration: none;\r\n        text-transform: uppercase;\r\n        font-size: 16px;\r\n        font-family: $content;\r\n        width: fit-content;\r\n    }\r\n}\r\n\r\nmain {\r\n    background-color: black;\r\n    background-image: url(img/stars.png);\r\n\r\n    .slogan {\r\n        @include grid;\r\n        height: 100vh;\r\n\r\n        .orbit {\r\n            width: 600px;\r\n            height: 600px;\r\n            margin: auto;\r\n            animation: updown 3s linear infinite;\r\n        }\r\n\r\n        ul:nth-child(3) li {\r\n            color: $text-color-W;\r\n            text: {\r\n                transform: uppercase;\r\n                align: center;\r\n            }\r\n\r\n            font: {\r\n                size: 1.5em;\r\n                family: $content;\r\n            }\r\n\r\n            cursor: default;\r\n            line-height: 120%;\r\n        }\r\n\r\n        &>p{\r\n            position: relative;\r\n            color: grey;\r\n            text: {\r\n                align: center;\r\n                transform: uppercase;\r\n            }\r\n            margin: 2em;\r\n            animation: opacity 3s ease-in infinite;\r\n        }\r\n\r\n        &>p::after {\r\n            content: \"\";\r\n            position: absolute;\r\n            width: 15px;\r\n            height: 15px;\r\n            border: {\r\n                bottom: 1px solid grey;\r\n                left: 1px solid grey;\r\n            }\r\n            bottom: 0px;\r\n            left: 50%;\r\n            transform: translate(-50%, 110%) rotate(-45deg);\r\n        }\r\n\r\n    }\r\n}"},"lineCount":null}},"error":null,"hash":"7b5428e3c138f48e1778d4aee1c6fff8","cacheData":{"env":{}}}